{"ast":null,"code":"import mapboxgl from \"mapbox-gl\";\nexport default {\n  name: \"HomeView\",\n\n  data() {\n    return {\n      map: null,\n      coordinates: [-111.549668, 39.014],\n      locations: [{\n        name: \"Brushy Mountain State Penitentiary\",\n        date: \"2019-10-26\",\n        coordinates: {\n          latitude: 36.1052,\n          longitude: -84.4529\n        }\n      }, {\n        name: \"Old South Pittsburg Hospital\",\n        date: \"2019-11-02\",\n        coordinates: {\n          latitude: 35.0021,\n          longitude: -85.7147\n        }\n      }],\n      accessToken: `${process.env.VUE_APP_API_KEY}`,\n      mapStyle: \"mapbox://styles/mapbox/streets-v11\"\n    };\n  },\n\n  mounted() {\n    mapboxgl.accessToken = this.accessToken;\n    const map = new mapboxgl.Map({\n      container: \"mapContainer\",\n      style: \"mapbox://styles/mapbox/streets-v11\",\n      center: [103.811279, 1.345399],\n      zoom: 12,\n      maxBounds: [[103.6, 1.1704753], [104.1, 1.4754753]]\n    });\n    new mapboxgl.Marker().setLngLat([103.811279, 1.345399]).addTo(map);\n  }\n\n};","map":{"version":3,"sources":["/Users/muhammad/Ayaz/express-vue3-tailwind-map/client/src/views/HomeView.vue"],"names":[],"mappings":"AAyBA,OAAO,QAAP,MAAqB,WAArB;AACA,eAAe;AACb,EAAA,IAAI,EAAE,UADO;;AAEb,EAAA,IAAI,GAAG;AACL,WAAO;AACL,MAAA,GAAG,EAAE,IADA;AAEL,MAAA,WAAW,EAAE,CAAC,CAAC,UAAF,EAAc,MAAd,CAFR;AAGL,MAAA,SAAS,EAAE,CACT;AACE,QAAA,IAAI,EAAE,oCADR;AAEE,QAAA,IAAI,EAAE,YAFR;AAGE,QAAA,WAAW,EAAE;AACT,UAAA,QAAQ,EAAE,OADD;AAET,UAAA,SAAS,EAAE,CAAC;AAFH;AAHf,OADS,EAUT;AACE,QAAA,IAAI,EAAE,8BADR;AAEE,QAAA,IAAI,EAAE,YAFR;AAGE,QAAA,WAAW,EAAE;AACT,UAAA,QAAQ,EAAE,OADD;AAET,UAAA,SAAS,EAAE,CAAC;AAFH;AAHf,OAVS,CAHN;AAsBL,MAAA,WAAW,EAAG,GAAE,OAAO,CAAC,GAAR,CAAY,eAAgB,EAtBvC;AAuBL,MAAA,QAAQ,EAAE;AAvBL,KAAP;AAyBD,GA5BY;;AA6Bb,EAAA,OAAO,GAAG;AACR,IAAA,QAAQ,CAAC,WAAT,GAAuB,KAAK,WAA5B;AAEA,UAAM,GAAE,GAAI,IAAI,QAAQ,CAAC,GAAb,CAAiB;AAC3B,MAAA,SAAS,EAAE,cADgB;AAE3B,MAAA,KAAK,EAAE,oCAFoB;AAG3B,MAAA,MA